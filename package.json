    "@storybook/addons": "^3.4.7",
    "@storybook/react": "3.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "coveralls": "^3.0.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.9.1",
    "jest": "^22.4.2",
    "json-loader": "^0.5.4",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.17",
    "prop-types": "^15.6.1",
    "istanbul": "^0.4.5",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "rimraf": "^2.5.4",
    "storybook-addon-jsx": "^4.1.1",
    "webpack": "^1.13.2"
  },
  "jest": {
    "testRegex": "/tests/.*",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "keywords": [
    "date",
    "moment",
    "react",
    "react-component",
    "time"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "moment": "^2.19.1",
    "prop-types": "^15.6.0",
    "react": "^15.6.0 || ^16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/headzoo/react-moment.git"
  },
  "scripts": {
    "build": "npm run clean && webpack -p",
    "clean": "rimraf ./dist/index.js",
    "test": "export TZ=America/New_York; jest --coverage",
    "travis-test": "npm run test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "eslint ./src --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook -o stories/build"
  },
  "types": "dist/index.d.ts"
}

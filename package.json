{
  "name": "react-moment",
  "description": "React component for the moment date library.",
  "version": "0.7.9",
  "author": {
    "name": "Sean Hickey",
    "web": "http://headzoo.io"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-knobs": "^3.4.7",
